<!-- News List -->
<div class="news_list">
	<div class="fs-row">
		<div class="fs-cell">
			<div class="news_list_inner">
				<ul class="news_list_items" id="item_list" aria-label="{{ page.title }}">
					{% for item in items %}
					<li class="news_list_item">
						{% include partial("news-item") with {
							item: item
						} %}
					</li>
					{% endfor %}
				</ul>
				{% include partial("pagination") with {
					title: "News Pagination",
					pages: "50"
				} %}
			</div>
		</div>
	</div>
</div>
<!-- END: News List -->